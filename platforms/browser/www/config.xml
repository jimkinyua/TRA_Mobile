<?xml version='1.0' encoding='utf-8'?>
<widget id="com.attainke.kdbmobile" version="2.0.3" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>KDBMobile</name>
    <description>
        A mobile Milk inspection application for the Kenya Dairy Board
    </description>
    <author email="ngugi.joseph@gmail.com" href="http://www.attain-es.com">
        Attain Enterprise Solutions
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="android-targetSdkVersion" value="24" />
</widget>
